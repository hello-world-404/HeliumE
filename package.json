{
    "name": "HeliumE",
    "version": "0.1.0",
    "author": "ydw",
    "description": "Helium toolbox",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package": "electron-packager ./ HeliumE --out ./output --app-version 0.1.0 --overwrite --icon ./icon.ico",
        "setup": "electron-installer-windows --src HeliumE-win32-x64/ --dest installer/ --config config.json --icon ./icon.ico"
    },
    "devDependencies": {
        "electron": "^12.0.5",
        "electron-installer-windows": "^3.0.0"
    },
    "dependencies": {
        "electron-squirrel-startup": "^1.0.0"
    }
}
